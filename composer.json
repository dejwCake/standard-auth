{
    "name": "dejw-cake/standard-auth",
    "description": "StandardAuth plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": [
        "auth",
        "users",
        "roles"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "DejwCake",
            "email": "github@dejw.sk",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0",
        "dejw-cake/adminlte": "dev-master",
        "dejw-cake/standard-cms": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "DejwCake\\StandardAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DejwCake\\StandardAuth\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
